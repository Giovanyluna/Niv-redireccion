    window.google = {};
    (function() {
      var d = /^\s*(?!javascript:)(?:[\w+.-]+:|[^:/?#]*(?:[/?#]|$))/i;
      (this || self).google.navigateTo = function(a, c, b) {
        !/\/.*?[&?]gsc=1/.test(c.location.href) &&
        a !== c &&
        a.google
          ? (a.google.r && (a.google.r = 0),
            (a = a.location),
            (b = d.test(b) ? b : void 0),
            b !== void 0 && (a.href = b),
            c.location.replace("about:blank"))
          : c.location.replace(b);
      };
    }).call(this);

